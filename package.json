{
  "name": "marketing-website",
  "version": "1.0.0",
  "description": "marketing website for a new tech product",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.prod.js",
    "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BernardoAguayoOrtega/Marketing-Website.git"
  },
  "keywords": [
    "react.js",
    "test",
    "webpack",
    "babel"
  ],
  "author": "Bernardo Aguayo Ortega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BernardoAguayoOrtega/Marketing-Website/issues"
  },
  "homepage": "https://github.com/BernardoAguayoOrtega/kMarketing-Website#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "jest-css-modules": "^2.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>./src/setupTest.js"
